pipeline {
    agent none
	 stages {
	    stage ('TESTING') {
		  parallel {
		      stage('Testing1') {
			    agent { label 'master'}
				steps {
				sh 'echo "This is Testing1 stage"
                sleep 5'
			  }
          	}
			  stage('Testing2') {
			     agent { label 'master' }
				  steps {
				   sh ' echo "This is Testing2 stage"
				    sleep 5'
					
			  }
			}
		  }
		}
	}
}	
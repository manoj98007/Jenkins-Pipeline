pipeline {
    agent none
	 stages {
	    stage ('TESTING') {
	    git 'https://github.com/manoj98007/Jenkins-Pipeline.git'
		  parallel {
		      stage('Testing1') {
			    agent { label 'master'}
				steps {
				sh 'echo "This is Testing1 stage"'
			  }
          	}
			  stage('Testing2') {
			     agent { label 'master' }
				  steps {
				   sh 'echo "This is Testing2 stage"'
					
			  }
		      }
		  }
	    }
	}
}	

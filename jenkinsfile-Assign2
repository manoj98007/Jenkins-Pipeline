    agent any
    stages {
            stage('ALL') {
                parallel {
                    
                    stage('Testing1') {
                        when {
                         branch 'master'
                        }
                        steps {
                           echo "This is Testing1 stage"
                         }
                    }
                    stage('Testing2') {
                        when {
                         branch 'master'
                        }
                        steps { 
                          echo "This is Testing2 stage"
                      }
                    }
                }
                
            }
        }
        
 }
